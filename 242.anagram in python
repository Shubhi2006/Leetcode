class Solution(object):
    def isAnagram(self, s, t):
        flag=True
        if(len(s)==len(t)):
            for i in s:
                if(s.count(i)==t.count(i)):
                    continue
                else:
                    flag=False
        else:
            return False
        
        if(flag==True):
            return True
        else:
            return False
        
