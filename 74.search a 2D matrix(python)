class Solution:
    def searchMatrix(self, matrix: List[List[int]], target: int) -> bool:
        m=len(matrix)
        n=len(matrix[0])
        low=0
        high=m*n-1
        while low<=high:
            mid=(low+high)//2
            row=mid//n
            col=mid%n
            if target==matrix[row][col]:
                return True
            if target <matrix[row][col]:
                high=mid-1
            else:
                low=mid+1
        else:
            return False
